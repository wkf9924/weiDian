#import <UIKit/UIKit.h>

@interface UIView (MHExtension)

@property (assign, nonatomic) CGFloat mh_x;
@property (assign, nonatomic) CGFloat mh_y;
@property (assign, nonatomic) CGFloat mh_width;
@property (assign, nonatomic) CGFloat mh_height;
@property (assign, nonatomic) CGSize mh_size;
@property (assign, nonatomic) CGPoint mh_origin;

@end
